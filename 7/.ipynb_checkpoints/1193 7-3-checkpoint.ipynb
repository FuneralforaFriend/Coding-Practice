   "source": [
    "X = int(input())\n",
    "\n",
    "n = 1\n",
    "while not(X <= n*(n+1)/2):\n",
    "    n += 1\n",
    "move_amount = X - (n - 1)*(n)/2 - 1\n",
    "print(int((n if n%2 != 0 else 1) + ((-1)**(n%2))*move_amount), end=\"/\")\n",
    "print(int((1 if n%2 != 0 else n) + ((-1)**(n%2+1))*move_amount))"
   ]
  }
