   "source": [
    "x,y,w,h = map(int, input().split())\n",
    "\n",
    "if w > h:\n",
    "    if (x < h) | (x > w - h):\n",
    "        print(min(x, w - x, y, h - y))\n",
    "    else:\n",
    "        print(min(y,h - y))\n",
    "else:\n",
    "    if (y < w) | (y > h - w):\n",
    "        print(min(y, h - y, x, w - x))\n",
    "    else:\n",
    "        print(min(x, w - x))"
   ]
  }
